{
  "compilerOptions": {
    "sourceMap": true,
    "module": "commonjs",
    "target": "ES5",
    "allowJs": true,
    "baseUrl": "src",
    "skipLibCheck": true,
    "declaration": true,
    "declarationDir": "./dev/dts",
    "outDir": "./dev/js/"
  },
  "typedocOptions": {
    "entryPointStrategy": "expand",
    "entryPoints": ["./src/Main.ts"],
    "sort": ["source-order"],
    "out": "./docs"
  }
}